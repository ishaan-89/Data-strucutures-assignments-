#include<bits/stdc++.h>
using namespace std;
void removeElement(int* arr, int& size, int pos){
    for(int i=pos;i<size-1;i++){
        arr[i]=arr[i+1];

    }
    size--;
    

}
int main(){
    int arr[]={1,2,1,1,4,5,5,9,0,0,10,10,100,120,120};
    int size = sizeof(arr)/sizeof(int);
    for(int i =0;i<size;i++){
        for(int j=i+1;j<size-1;j++){
            if(arr[i]==arr[j]){
                removeElement(arr, size, j);
                j--;

            }
        }
    }
    cout<<"array after deletion duplicate elements : ";
    for(int i=0;i<size;i++){
        cout<<arr[i]<<" ";

    }
    return 0;

}
