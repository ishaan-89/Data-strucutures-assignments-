#include<bits/stdc++.h>
using namespace std;
int findmissing(int* arr, int size){
    for(int i=0;i<size;i++){
        if(arr[i]-i!=1){
            return i+1;
            
        }
    }
}
int main(){
    int arr[]={1,2,3,5,6};
    int size = sizeof(arr)/sizeof(int);
    int ans = findmissing(arr, size);
    cout<<" missing is : "<<ans<<endl;
    return 0;

}
