#include <iostream>
#include <stack>
#include <cctype>
using namespace std;

int solvePostfix(const string& exp) {
    stack<int> s;

    for (int i = 0; i < exp.length(); i++) {
        char c = exp[i];

        if (isdigit(c)) {
            s.push(c - '0');  
        } 
        else {
            int b = s.top(); s.pop();
            int a = s.top(); s.pop();

            switch (c) {
                case '+': s.push(a + b); break;
                case '-': s.push(a - b); break;
                case '*': s.push(a * b); break;
                case '/': s.push(a / b); break;
            }
        }
    }
    return s.top();
}

int main() {
    string postfix;
    cout << "Enter a postfix expression: ";
    cin >> postfix;

    int answer = solvePostfix(postfix);
    cout << "Evaluation Result: " << answer << endl;

    return 0;
}
