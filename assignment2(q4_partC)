#include<bits/stdc++.h>
using namespace std;
void removeChar(string&s, int pos){
    if (pos >= s.size()) return;

        for(int i=pos;i<s.size()-1;i++){
            s[i]=s[i+1];

        }
        s.pop_back();

    

}

int main(){
    string s = "chitransh";
 // vowels are a , e, i, o, u

 for(int i=0;i<s.size();i++){
    if(s[i]>='A' && s[i]<='Z'){
        if(s[i]=='A' || s[i]=='E' || s[i]=='I' || s[i]=='O' || s[i]=='U'){
            removeChar(s, i);
            i--;

        }
    }
       else{
         if(s[i]=='a' || s[i]=='e' || s[i]=='i' || s[i]=='o' || s [i]=='u'){
            removeChar(s, i);
            i--;


        }
    }
    
 }

 cout<<"the string after removing vowels : "<<s;
return 0;

}
